<!--pages/document/show/show.wxml-->
<page-meta root-font-size="system"/>
<view class="page" data-weui-theme="{{theme}}" data-weui-mode="{{mode}}">
  <view class="page__hd">
      <view class="page__title">部分材料示例</view>
      <view class="page__desc">展示一些符合要求的材料</view>
  </view>
  <view class="page__bd">
  <view class="weui-grids">
		<navigator class="weui-grid" aria-role="button" url="javascript:" bindtap="open1">
			<view class="weui-grid__icon">
				<mp-icon icon="copy" color="black" size="{{25}}"></mp-icon>
			</view>
			<view class="weui-grid__label">身份证翻译件</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:" bindtap="open2">
			<view class="weui-grid__icon">
				<mp-icon icon="copy" color="black" size="{{25}}"></mp-icon>
			</view>
			<view class="weui-grid__label">在读证明</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:" bindtap="open3">
			<view class="weui-grid__icon">
				<mp-icon icon="copy" color="black" size="{{25}}"></mp-icon>
			</view>
			<view class="weui-grid__label">CMA成绩单</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:" bindtap="open4">
			<view class="weui-grid__icon">
				<mp-icon icon="copy" color="black" size="{{25}}"></mp-icon>
			</view>
			<view class="weui-grid__label">CMA工作经验</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:" bindtap="open5">
			<view class="weui-grid__icon">
				<mp-icon icon="copy" color="black" size="{{25}}"></mp-icon>
			</view>
			<view class="weui-grid__label">CPA</view>
		</navigator>
		<navigator class="weui-grid" aria-role="button" url="javascript:" bindtap="open6">
			<view class="weui-grid__icon">
				<mp-icon icon="copy" color="black" size="{{25}}"></mp-icon>
			</view>
			<view class="weui-grid__label">加速信</view>
		</navigator>
	</view>
</view>
  <view class="button-sp-area cell">
			<navigator class="weui-btn_cell weui-btn_cell-primary" aria-role="button" url="/pages/exemption/exemption">有意向申请免试？</navigator>
  </view>
  <view wx:if="{{ID}}" aria-role="dialog" aria-modal="true" tabindex="0" class="weui-gallery fadeIn" bindtap="close1">
    <view class="weui-gallery__info">1/6</view>
    <view class="weui-gallery__img__wrp">
        <image mode="aspectFit" class="weui-gallery__img" src="https://s1.ax1x.com/2022/12/17/zHLo8K.png"></image>
    </view>
  </view>
  <view wx:if="{{Enrollment}}" aria-role="dialog" aria-modal="true" tabindex="0" class="weui-gallery fadeIn" bindtap="close2">
    <view class="weui-gallery__info">2/6</view>
    <view class="weui-gallery__img__wrp">
        <image mode="aspectFit" class="weui-gallery__img" src="https://s1.ax1x.com/2022/12/17/zHLzPP.png"></image>
    </view>
  </view>
  <view wx:if="{{CMA_Trans}}" aria-role="dialog" aria-modal="true" tabindex="0" class="weui-gallery fadeIn" bindtap="close3">
    <view class="weui-gallery__info">3/6</view>
    <view class="weui-gallery__img__wrp">
        <image mode="aspectFit" class="weui-gallery__img" src="https://s1.ax1x.com/2022/12/17/zHOPKg.png"></image>
    </view>
  </view>
  <view wx:if="{{Working_Exp}}" aria-role="dialog" aria-modal="true" tabindex="0" class="weui-gallery fadeIn" bindtap="close4">
    <view class="weui-gallery__info">4/6</view>
    <view class="weui-gallery__img__wrp">
        <image mode="aspectFit" class="weui-gallery__img" src="https://s1.ax1x.com/2022/12/17/zHOirQ.png"></image>
    </view>
  </view>
  <view wx:if="{{CPA}}" aria-role="dialog" aria-modal="true" tabindex="0" class="weui-gallery fadeIn" bindtap="close5">
    <view class="weui-gallery__info">5/6</view>
    <view class="weui-gallery__img__wrp">
        <image mode="aspectFit" class="weui-gallery__img" src="https://s1.ax1x.com/2022/12/17/zHOFbj.png"></image>
    </view>
  </view>
  <view wx:if="{{Accelerate}}" aria-role="dialog" aria-modal="true" tabindex="0" class="weui-gallery fadeIn" bindtap="close6">
    <view class="weui-gallery__info">6/6</view>
    <view class="weui-gallery__img__wrp">
        <image mode="aspectFit" class="weui-gallery__img" src="https://s1.ax1x.com/2022/12/17/zHOAVs.png"></image>
    </view>
  </view>
</view>
